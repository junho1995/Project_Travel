document.addEventListener('DOMContentLoaded', function() {
    renderCalendar();
    
    function initMap() {
    // 지도의 중심 좌표를 설정
    var myLatLng = {lat: latitudes[0], lng: longitudes[0]};
    // 지도 생성
    var map = new google.maps.Map(document.getElementById('map'), {
        center: myLatLng,
        zoom: 8 // 줌 레벨을 조정할 수 있습니다.
        });
    };
});